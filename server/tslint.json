{
  "defaultSeverity": "error",
  "extends": ["../tslint.json"],
  "jsRules": {},
  "rules": {
    "function-name": [
      true,
      {
        "method-regex": "^[a-z$][\\w\\d]+$",
        "private-method-regex": "^[a-z][\\w\\d]+$",
        "protected-method-regex": "^[a-z][\\w\\d]+$",
        "static-method-regex": "^[a-zA-Z_\\d]+$",
        "function-regex": "^[a-z][\\w\\d]+$"
      }
    ]
  },
  "rulesDirectory": [],
  "linterOptions": {
    "exclude": ["src/migrations/*.ts", "**/*.json"]
  }
}
